APPLICATION_LAYER_WEB_IMAGE=ceyoniq.azurecr.io/release/nscale/application-layer-web:8.4.1100.2022102415.0
CONSOLE_IMAGE=ceyoniq.azurecr.io/release/nscale/console:8.4.1001.12664.0
MONITORING_CONSOLE_IMAGE=ceyoniq.azurecr.io/release/nscale/monitoring-console:8.4.1001.2022091718.0
RENDITION_SERVER_IMAGE=ceyoniq.azurecr.io/release/nscale/rendition-server:8.4.1100.2022101418.0
STORAGE_LAYER_IMAGE=ceyoniq.azurecr.io/release/nscale/storage-layer:8.4.1100.2022102415.0
PIPELINER_IMAGE=ceyoniq.azurecr.io/release/nscale/pipeliner:8.4.1100.2022102112.0
ADMINISTRATOR_IMAGE=ceyoniq.azurecr.io/release/nscale/administrator:8.4.1101.2022102512.0

# additional connector images
CMIS_CONNECTOR=ceyoniq.azurecr.io/release/nscale/cmis-connector:8.4.1100.2022102016.0
ILM_CONNECTOR=ceyoniq.azurecr.io/release/nscale/ilm-connector:8.4.1001.2022091517.0
WEBDAV_CONNECTOR=ceyoniq.azurecr.io/release/nscale/webdav-connector:8.4.1001.2022091517.0
XTA_CONNECTOR=ceyoniq.azurecr.io/release/nscale/xta-connector:8.4.1001.2022091517.0

# third party images
POSTGRESQL_IMAGE=bitnami/postgresql:14
LOKI_IMAGE=grafana/loki:latest
GRAFANA_IMAGE=grafana/grafana:latest
PROMETHEUS_IMAGE=prom/prometheus:latest
CADVISOR_IMAGE=gcr.io/google-containers/cadvisor:latest
TRAEFIK_IMAGE=traefik:v2.8
TRAEFIK_EXTERNAL_HTTP_PORT=80
TRAEFIK_EXTERNAL_HTTPS_PORT=443

COMPOSE_PATH_SEPARATOR=:

COMPOSE_FILE=docker-compose.yml:docker-compose.proxy.yml:docker-compose.default-ports.yml

#
# To activate additional features, use the following configuration files.
#
# Example:
# nscale with HTTPS support,Loki, Prometheus and Grafana:
# COMPOSE_FILE=docker-compose.yml:docker-compose.proxy-with-ssl.yml:docker-compose.analytics.yml
#
#
# Proxy support without SSL      docker-compose.proxy.yml
# Proxy support with SSL         docker-compose.proxy-with-ssl.yml
# default ports                  docker-compose.default-ports.yml
# nscale Pipeliner               docker-compose.pipeliner.yml
# nscale Connectors              docker-compose.connectors.yml
# Prometheus, Loki and Grafana   docker-compose.analytics.yml
# 
