APPLICATION_LAYER_WEB_IMAGE=ceyoniq.azurecr.io/release/nscale/application-layer-web:ubi.9.0.1200.2023052511
CONSOLE_IMAGE=ceyoniq.azurecr.io/release/nscale/console:ubi.9.0.1200.13068
MONITORING_CONSOLE_IMAGE=ceyoniq.azurecr.io/release/nscale/monitoring-console:ubi.9.0.1200.2023051908
RENDITION_SERVER_IMAGE=ceyoniq.azurecr.io/release/nscale/rendition-server:ubi.9.0.1200.2023051618
STORAGE_LAYER_IMAGE=ceyoniq.azurecr.io/release/nscale/storage-layer:ubi.9.0.1200.2023052408
PIPELINER_IMAGE=ceyoniq.azurecr.io/release/nscale/pipeliner:ubi.9.0.1200.2023052211
ADMINISTRATOR_IMAGE=ceyoniq.azurecr.io/release/nscale/administrator:ubi.9.0.1200.2023052416
PROCESS_AUTOMATION_MODELER_IMAGE=ceyoniq.azurecr.io/release/nscale/process-automation-modeler:ubi.9.0.1000.2023040316

# additional connector images
CMIS_CONNECTOR=ceyoniq.azurecr.io/release/nscale/cmis-connector:ubi.9.0.1202.2023052608
ILM_CONNECTOR=ceyoniq.azurecr.io/release/nscale/ilm-connector:ubi.9.0.1200.2023051702
WEBDAV_CONNECTOR=ceyoniq.azurecr.io/release/nscale/webdav-connector:ubi.9.0.1200.2023051702
XTA_CONNECTOR=ceyoniq.azurecr.io/release/nscale/xta-connector:ubi.9.0.1200.2023051716

# third party images
POSTGRESQL_IMAGE=docker.io/bitnami/postgresql:14
LOKI_IMAGE=grafana/loki:latest
GRAFANA_IMAGE=grafana/grafana:latest
PROMETHEUS_IMAGE=prom/prometheus:latest
CADVISOR_IMAGE=gcr.io/google-containers/cadvisor:latest
TRAEFIK_IMAGE=traefik:v2.8
TRAEFIK_EXTERNAL_HTTP_PORT=80
TRAEFIK_EXTERNAL_HTTPS_PORT=443

COMPOSE_PATH_SEPARATOR=:

# default project name
# can be derived to manage different compose configuration from the same project folder
COMPOSE_PROJECT_NAME=nscale
COMPOSE_PATH_SEPARATOR=:
COMPOSE_FILE=docker-compose.yml:docker-compose.setup.yml:docker-compose.default-health.yml:docker-compose.default-ports.yml:docker-compose.proxy.yml

#
# To activate additional features, use the following configuration files.
#
# Example:
# nscale with HTTPS support,Loki, Prometheus and Grafana:
# COMPOSE_FILE=docker-compose.yml:docker-compose.proxy-with-ssl.yml:docker-compose.analytics.yml
#
#
# Setup initial document area           docker-compose.setup.yml
# Proxy support without SSL             docker-compose.proxy.yml
# Proxy support with SSL                docker-compose.proxy-with-ssl.yml
# default ports                         docker-compose.default-ports.yml
# nscale Pipeliner                      docker-compose.pipeliner.yml
# nscale Process Automation Modeler     docker-compose.modeler.yml
# nscale Connectors                     docker-compose.connectors.yml
# Prometheus, Loki and Grafana          docker-compose.analytics.yml
# 
