CONSOLE_IMAGE=ceyoniq.azurecr.io/release/nscale/console:8.4.1401.12950
MONITORING_CONSOLE_IMAGE=ceyoniq.azurecr.io/release/nscale/monitoring-console:8.4.1400.2023011609
RENDITION_SERVER_IMAGE=ceyoniq.azurecr.io/release/nscale/rendition-server:8.4.1400.2023011609
STORAGE_LAYER_IMAGE=ceyoniq.azurecr.io/release/nscale/storage-layer:8.4.1400.2023011810
PIPELINER_IMAGE=ceyoniq.azurecr.io/release/nscale/pipeliner:8.4.1402.2023013108
