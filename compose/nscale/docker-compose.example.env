CONSOLE_IMAGE=ceyoniq.azurecr.io/release/nscale/console:ubi.10.0.1501.13429
MONITORING_CONSOLE_IMAGE=ceyoniq.azurecr.io/release/nscale/monitoring-console:ubi.10.0.1400.2025080112
RENDITION_SERVER_IMAGE=ceyoniq.azurecr.io/release/nscale/rendition-server:ubi.10.0.1500.2025082118
STORAGE_LAYER_IMAGE=ceyoniq.azurecr.io/release/nscale/storage-layer:ubi.10.0.1400.112648
PIPELINER_IMAGE=ceyoniq.azurecr.io/release/nscale/pipeliner:ubi.10.0.1502.2025082716
ADMINISTRATOR_IMAGE=ceyoniq.azurecr.io/release/nscale/administrator:ubi.10.0.1400
PROCESS_AUTOMATION_MODELER_IMAGE=ceyoniq.azurecr.io/release/nscale/process-automation-modeler:ubi.10.0.1400.103179

# additional connector images
CMIS_CONNECTOR=ceyoniq.azurecr.io/release/nscale/cmis-connector:ubi.10.0.1500.2025082520
ERP_CMIS_CONNECTOR=ceyoniq.azurecr.io/release/nscale/erp-cmis-connector:ubi.10.0.1500.2025082510
ILM_CONNECTOR=ceyoniq.azurecr.io/release/nscale/erp-ilm-connector:ubi.10.0.1500.2025082510
